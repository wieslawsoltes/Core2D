      <MenuItem Header="Import _Json..." Command="{Binding Platform.OnImportJson, FallbackValue={x:Null}}" />
      <Separator />
      <MenuItem Header="Import Sv_g..." Command="{Binding Platform.OnImportSvg, FallbackValue={x:Null}}" />
      <Separator />
      <MenuItem Header="_Export..." Command="{Binding Platform.OnExport, FallbackValue={x:Null}}" InputGesture="Ctrl+E" />
      <Separator IsVisible="{Binding !!RecentProjects.Length}" />
      <MenuItem Header="_Recent" Items="{Binding RecentProjects}" IsVisible="{Binding !!RecentProjects.Length}">
