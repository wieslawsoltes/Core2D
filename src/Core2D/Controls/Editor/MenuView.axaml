        <Separator />
        <MenuItem Header="E_xport Page" Command="{Binding Platform.OnExport, FallbackValue={x:Null}}" CommandParameter="{Binding Project.CurrentContainer, FallbackValue={x:Null}}" />
      </MenuItem>
