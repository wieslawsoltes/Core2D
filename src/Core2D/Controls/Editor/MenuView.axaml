          <MenuItem.Icon>
            <CheckBox IsChecked="{Binding Project.Options.TryToConnect, FallbackValue={x:Null}}" />
          </MenuItem.Icon>
        </MenuItem>
      </MenuItem>
      <Separator />
      <MenuItem Header="E_xport Project" Command="{Binding Platform.OnExport, FallbackValue={x:Null}}" CommandParameter="{Binding Project}" />
    </MenuItem>
