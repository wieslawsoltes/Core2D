        <Separator />
        <MenuItem Header="_Delete Layer" Command="{Binding Project.RemoveLayer, FallbackValue={x:Null}}" CommandParameter="{Binding Project.CurrentContainer.CurrentLayer, FallbackValue={x:Null}}" />
