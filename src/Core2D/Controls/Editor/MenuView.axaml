        <Separator />
        <MenuItem Header="Object Browser" Command="{Binding OnToggleDockableVisibility}" CommandParameter="ObjectBrowser" />
      </MenuItem>
      <MenuItem Header="_Appearance" IsVisible="{Binding !!RootDock}">
        <MenuItem Header="_Main Menu" Command="{Binding OnToggleDockableVisibility}" CommandParameter="HomeMenuView" />
        <MenuItem Header="_Status Bar" Command="{Binding OnToggleDockableVisibility}" CommandParameter="HomeStatusBarView" />
      </MenuItem>
      <MenuItem Header="_Zoom">
