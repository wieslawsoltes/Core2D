        <MenuItem Header="Insert Document Aft_er" Command="{Binding Project.OnInsertDocumentAfter, FallbackValue={x:Null}}" CommandParameter="{Binding Project.CurrentDocument, FallbackValue={x:Null}}" />
        <Separator />
        <MenuItem Header="Cu_t Document" Command="{Binding ClipboardService.OnCut, FallbackValue={x:Null}}" CommandParameter="{Binding Project.CurrentDocument, FallbackValue={x:Null}}" />
        <MenuItem Header="_Copy Document" Command="{Binding ClipboardService.OnCopy, FallbackValue={x:Null}}" CommandParameter="{Binding Project.CurrentDocument, FallbackValue={x:Null}}" />
