                  CommandParameter="{Binding Project.CurrentDatabase, FallbackValue={x:Null}}" />
      </MenuItem>
      <MenuItem Header="O_ptions">
        <MenuItem Header="Default is Stro_ked" Command="{Binding OnToggleDefaultIsStroked}" InputGesture="K">
          <MenuItem.Icon>
            <CheckBox IsChecked="{Binding Project.Options.DefaultIsStroked, FallbackValue={x:Null}}" />
          </MenuItem.Icon>
        </MenuItem>
        <MenuItem Header="Default is _Filled" Command="{Binding OnToggleDefaultIsFilled}" InputGesture="F">
          <MenuItem.Icon>
            <CheckBox IsChecked="{Binding Project.Options.DefaultIsFilled, FallbackValue={x:Null}}" />
          </MenuItem.Icon>
        </MenuItem>
        <Separator />
        <MenuItem Header="Default is Close_d" Command="{Binding OnToggleDefaultIsClosed}" InputGesture="D">
          <MenuItem.Icon>
            <CheckBox IsChecked="{Binding Project.Options.DefaultIsClosed, FallbackValue={x:Null}}" />
          </MenuItem.Icon>
        </MenuItem>
        <Separator />
